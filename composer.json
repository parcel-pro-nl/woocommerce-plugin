  "type": "wordpress-plugin",
  "version": "1.9.0",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-intl": "*"
  },
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Parcel Pro",
      "email": "ict@parcelpro.nl",
      "homepage": "https://www.parcelpro.nl",
      "role": "Developer"
    }
